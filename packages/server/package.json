  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js",
    "models-user": "npx sequelize-cli model:generate --name User --attributes first_name:string,last_name:string,email:string,username:string",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "migrate:rollback": "npx sequelize-cli db:migrate:undo",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all",
    "seeder-generate": "npx sequelize seed:generate --name default-root",
    "seed:all": "npx sequelize db:seed:all"
  },
